<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Status</title>
</head>
<body>
{{$ID := .ProID}}
<table border="2px"  width="70%" align="center">
    <tbody>
        <tr bgcolor="aqua" align="center">
            <td width="20%">Submit Time</td>
            <td width="20%">Judge Status</td>
            <td width="20%">RunTime</td>
            <td width="20%">RunMemory</td>
            <td width="20%">Details</td>
        </tr>
    {{range .Logs}}
        <tr align="center">
            <td><font size="5px">{{.SubTime}}</font></td>
            <td><font color="{{.Col}}" size="5px">{{.Status}}</font></td>
            <td><font size="5px" color="#5f9ea0">{{.Time}}</font></td>
            <td><font size="5px" color="#5f9ea0">{{.Mem}}</font></td>
            <td><a href="/details?ProID={{$ID}}&Dir={{.DirName}}">Details</a></td>
        </tr>
    {{end}}
    </tbody>
</table>
</body>
</html>